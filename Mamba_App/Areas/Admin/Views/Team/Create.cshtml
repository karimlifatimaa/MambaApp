@using Mamba.Core.Models;
@model Team
<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label asp-for="FullName" class="form-label">FullName</label>
            <input asp-for="FullName" class="form-control">
            <span asp-validation-for="FullName" style="color:red"></span>

        </div>
        <div class="mb-3">
            <label asp-for="Position" class="form-label">Position</label>
            <input asp-for="Position" class="form-control">
            <span asp-validation-for="Position" style="color:red"></span>

        </div>
        <div class="mb-3">
            <label asp-for="formFile" class="form-label">Image</label>
            <input asp-for="formFile" class="form-control">
            <span asp-validation-for="formFile" style="color:red"></span>

        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
